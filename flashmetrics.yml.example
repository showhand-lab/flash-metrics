
tidb:
  address: 0.0.0.0:4000

web:
  address: 0.0.0.0:9977

logs:
  log_level: info
#  log_file: flashmetrics.log

scrape_configs:
  - job_name: default job
    scrape_interval: 10s
    scrape_timeout: 3s
    metrics_path: /metrics
    scheme: http
    static_configs:
      - targets: ["127.0.0.1:8234"]
      - targets: ["127.0.0.1:10080"]
